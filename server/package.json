{
  "name": "session-tracker-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:mssql": "tsx watch src/index-mssql.ts",
    "build": "tsc",
    "build:mssql": "tsc src/index-mssql.ts --outDir dist --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck --target ES2020 --module CommonJS",
    "start": "node dist/index.js",
    "start:mssql": "node dist/index-mssql.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "mssql": "^10.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mssql": "^9.1.1",
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
